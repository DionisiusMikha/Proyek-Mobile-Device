                android:layout_height="99dp">

                <ImageView
                    android:id="@+id/tvAnalysis"
                    android:layout_width="55dp"
                    android:layout_height="55dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
