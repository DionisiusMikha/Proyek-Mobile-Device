<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.DanaDaruratFragment">

    <ScrollView
        android:id="@+id/scrollView3"
        android:layout_width="0dp"
        android:layout_height="674dp"
        android:layout_marginTop="132dp"
        android:background="@drawable/rounded_top_corners"
        android:paddingBottom="110dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@drawable/rounded_top_corners">

            <ImageView
                android:id="@+id/imageView4"
                android:layout_width="59dp"
                android:layout_height="70dp"
                android:layout_marginStart="32dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/penguin__2__1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView26"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/rounded_corner_background_pengu"
                android:padding="5dp"
                android:text="Yuk Ceritain Mimpimu"
                android:textColor="#000000"
                android:textStyle="bold"
                android:visibility="visible"
                app:layout_constraintStart_toEndOf="@+id/imageView4"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:background="@drawable/rounded_corner_background_pengu"
                android:padding="5dp"
                android:text="pengu akan bantuin kamu "
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView26"
                app:layout_constraintTop_toBottomOf="@+id/textView26" />

            <TextView
                android:id="@+id/textView41"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="Pengeluaran wajibmu setiap bulan?"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/imageView4"
                app:layout_constraintTop_toBottomOf="@+id/textView29" />

            <EditText
                android:id="@+id/etPengeluaranWajibTiapBulan"
                android:layout_width="280dp"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corner_background"
                android:ems="10"
                android:inputType="text"
                android:padding="15dp"
                app:layout_constraintStart_toStartOf="@+id/textView41"
                app:layout_constraintTop_toBottomOf="@+id/textView41" />

            <TextView
                android:id="@+id/textView42"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="apakah kamu sudah menikah?"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/etPengeluaranWajibTiapBulan"
                app:layout_constraintTop_toBottomOf="@+id/etPengeluaranWajibTiapBulan" />

            <Button
                android:id="@+id/btnSudahMenikah"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="6dp"
                android:backgroundTint="#DEF6E1"
                android:text="Sudah Menikah"
                android:textColor="#000000"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="@+id/textView42"
                app:layout_constraintTop_toBottomOf="@+id/textView42" />

            <Button
                android:id="@+id/btnBelumMenikah"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="28dp"
                android:backgroundTint="#DEF6E1"
                android:text="Belum Menikah"
                android:textColor="#000000"
                android:textSize="12sp"
                app:layout_constraintStart_toEndOf="@+id/btnSudahMenikah"
                app:layout_constraintTop_toTopOf="@+id/btnSudahMenikah" />

            <TextView
                android:id="@+id/textView51"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="Jumlah tanggunganmu?"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView42"
                app:layout_constraintTop_toBottomOf="@+id/btnSudahMenikah" />

            <EditText
                android:id="@+id/etJumlahTanggungan"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corner_background"
                android:ems="10"
                android:inputType="text"
                android:padding="15dp"
                app:layout_constraintStart_toStartOf="@+id/textView51"
                app:layout_constraintTop_toBottomOf="@+id/textView51" />

            <TextView
                android:id="@+id/textView52"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="24dp"
                android:text="Rp"
                android:textColor="#000000"
                app:layout_constraintStart_toEndOf="@+id/etPengeluaranWajibTiapBulan"
                app:layout_constraintTop_toTopOf="@+id/etPengeluaranWajibTiapBulan" />

            <TextView
                android:id="@+id/textView53"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="24dp"
                android:text="Orang"
                android:textColor="#000000"
                app:layout_constraintStart_toEndOf="@+id/etJumlahTanggungan"
                app:layout_constraintTop_toBottomOf="@+id/textView51" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="59dp"
                android:layout_height="70dp"
                android:layout_marginTop="28dp"
                android:src="@drawable/penguin__2__2"
                app:layout_constraintStart_toStartOf="@+id/etJumlahTanggungan"
                app:layout_constraintTop_toBottomOf="@+id/etJumlahTanggungan" />

            <TextView
                android:id="@+id/textView54"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/rounded_corner_background_pengu"
                android:padding="5dp"
                android:text="Thank you"
                android:textColor="#000000"
                app:layout_constraintStart_toEndOf="@+id/imageView"
                app:layout_constraintTop_toBottomOf="@+id/etJumlahTanggungan" />

            <TextView
                android:id="@+id/textView55"
                android:layout_width="231dp"
                android:layout_height="0dp"
                android:layout_marginTop="8dp"
                android:background="@drawable/rounded_corner_background_pengu"
                android:padding="5dp"
                android:text="Next, Ayo Atur Strategimu Investasi Kamu"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/textView54"
                app:layout_constraintTop_toBottomOf="@+id/textView54" />

            <TextView
                android:id="@+id/textView56"
                android:layout_width="251dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="Berapa lama targetmu mengumpulkan  dana darurat?"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/imageView"
                app:layout_constraintTop_toBottomOf="@+id/textView55" />

            <EditText
                android:id="@+id/etTargetBulanDanaDarurat"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corner_background"
                android:ems="10"
                android:inputType="text"
                android:padding="15dp"
                app:layout_constraintStart_toStartOf="@+id/textView56"
                app:layout_constraintTop_toBottomOf="@+id/textView56" />

            <TextView
                android:id="@+id/textView57"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="24dp"
                android:text="Bulan"
                android:textColor="#000000"
                app:layout_constraintStart_toEndOf="@+id/etTargetBulanDanaDarurat"
                app:layout_constraintTop_toTopOf="@+id/etTargetBulanDanaDarurat" />

            <TextView
                android:id="@+id/textView58"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="jumlah dana darurat yang kamu miliki saat ini?"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/etTargetBulanDanaDarurat"
                app:layout_constraintTop_toBottomOf="@+id/etTargetBulanDanaDarurat" />

            <EditText
                android:id="@+id/etJumlahDanaDaruratSaatIni"
                android:layout_width="240dp"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corner_background"
                android:ems="10"
                android:inputType="text"
                android:padding="15dp"
                app:layout_constraintStart_toStartOf="@+id/textView58"
                app:layout_constraintTop_toBottomOf="@+id/textView58" />

            <TextView
                android:id="@+id/textView59"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="24dp"
                android:text="Rp"
                android:textColor="#000000"
                app:layout_constraintStart_toEndOf="@+id/etJumlahDanaDaruratSaatIni"
                app:layout_constraintTop_toTopOf="@+id/etJumlahDanaDaruratSaatIni" />

            <TextView
                android:id="@+id/textView60"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="Target Investasi kamu setiap bulan"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/etJumlahDanaDaruratSaatIni"
                app:layout_constraintTop_toBottomOf="@+id/etJumlahDanaDaruratSaatIni" />

            <EditText
                android:id="@+id/etTargetInvestasiDanaDarurat"
                android:layout_width="240dp"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corner_background"
                android:ems="10"
                android:inputType="text"
                android:padding="15dp"
                app:layout_constraintStart_toStartOf="@+id/textView60"
                app:layout_constraintTop_toBottomOf="@+id/textView60" />

            <TextView
                android:id="@+id/textView61"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="24dp"
                android:text="Rp"
                android:textColor="#000000"
                app:layout_constraintStart_toEndOf="@+id/etTargetInvestasiDanaDarurat"
                app:layout_constraintTop_toTopOf="@+id/etTargetInvestasiDanaDarurat" />

            <TextView
                android:id="@+id/textView62"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:text="kamu akan investasi di produk yang returnya"
                android:textColor="#000000"
                app:layout_constraintStart_toStartOf="@+id/etTargetInvestasiDanaDarurat"
                app:layout_constraintTop_toBottomOf="@+id/etTargetInvestasiDanaDarurat" />

            <EditText
                android:id="@+id/etProdukReturDanaDarurat"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_corner_background"
                android:ems="10"
                android:inputType="text"
                android:padding="15dp"
                app:layout_constraintStart_toStartOf="@+id/textView62"
                app:layout_constraintTop_toBottomOf="@+id/textView62" />

            <TextView
                android:id="@+id/textView63"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="8dp"
                android:layout_marginTop="24dp"
                android:text="% / Tahun"
                android:textColor="#000000"
                app:layout_constraintStart_toEndOf="@+id/etProdukReturDanaDarurat"
                app:layout_constraintTop_toTopOf="@+id/etProdukReturDanaDarurat" />

            <Button
                android:id="@+id/btnHitungDanaDarurat"
                android:layout_width="160dp"
                android:layout_height="wrap_content"
                android:layout_marginTop="24dp"
                android:backgroundTint="#00CF9D"
                android:text="Hitung"
                app:cornerRadius="20dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/etProdukReturDanaDarurat" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </ScrollView>

    <TextView
        android:id="@+id/textView64"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dana Darurat"
        android:textColor="#000000"
        android:textSize="34sp"
        app:layout_constraintBottom_toTopOf="@+id/scrollView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>